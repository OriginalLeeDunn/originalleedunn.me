{
  "name": "originalleedunn.me",
  "version": "1.0.0",
  "private": true,
  "description": "Portfolio website for OriginalLeeDunn - AI & Game Developer",
  "author": "OriginalLeeDunn <hello@originalleedunn.me>",
  "homepage": "https://originalleedunn.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/OriginalLeeDunn/originalleedunn.me.git"
  },
  "keywords": [
    "portfolio",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "framer-motion"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "npm run optimize:images && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "preview": "next build && next start",
    "optimize:images": "node scripts/optimize-images.js",
    "generate:webp": "node scripts/create-webp-fallbacks.js",
    "optimize": "npm run optimize:images && npm run generate:webp",
    "docker:build": "docker build -t originalleedunn-me .",
    "docker:run": "docker run -p 3001:3001 originalleedunn-me",
    "docker:compose": "docker-compose up --build",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "new-post": "ts-node --project tsconfig.scripts.json scripts/new-post.ts",
    "blog:validate": "ts-node --project tsconfig.scripts.json scripts/validate-posts.ts",
    "blog:generate-images": "ts-node --project tsconfig.scripts.json scripts/generate-placeholder-images.ts",
    "blog:setup": "npm run blog:validate && npm run blog:generate-images",
    "blog:check-setup": "ts-node --project tsconfig.scripts.json scripts/check-blog-setup.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
